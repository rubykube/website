    <section class="resume-section p-3 p-lg-5 d-flex flex-column section-scroll" id="blog" data-nav-color="red">
      <div class="my-auto">
        <div class="container">
          <div class="row">
            <h1 class="mb-5" id="blog">
              <%= data.i18n.en.sections.blog.title %>
            </h1>
          </div>
          <div class="row">
              <% page_articles.sort_by{|a| a.date}.reverse.each_with_index do |article, i| %>
                <% if (i < 4) %>
                  <article class="col-sm-12 col-md-6">
                      <div class="main-blog-post">
                        <header>
                          <h2 class="post-title">
                            <span id="title-post"><%= link_to article.title, article %></span>
                          </h2>
                          <span class="main-post-category">
                            <%= article.data.category %> |
                          </span>
                          <span class="post-date">
                            <%= article.date.strftime('%b %e, %Y') %>
                          </span>
                        </header>
                        <div class="post-body preview"><%= article.summary(140, '...') %></div>
                      </div>
                  </article>
                <% end %>
              <% end %>
            </article>
          </div>
          <div class="d-flex">
            <div class="text-center">
              <a href="/blog" class="btn btn-primary btn-main-blog btn-blue">More posts</a>
            </div>
          </div>
        </div>
      </div>
    </section>


